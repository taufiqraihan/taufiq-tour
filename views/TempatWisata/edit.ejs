<% include ../partials/header-crud%>
<div class="container">
  <div class="">
    <h1 style="text-align:center;">Edit Data Tempat Wisata</h1>
    <div style="width:30%;margin: 25px auto;">
      <form class="" action="/TempatWisatas/edit/<%= TempatWisata.id %>" method="post">
        <div class="form-group">
          <label for="">Destinasi</label>
          <input class="form-control" type="text" name="destinasi" value="<%= TempatWisata.destinasi %>">
        </div>
        <div class="form-group">
          <label for="">Paket</label>
          <select class="form-control" type="text" name="paket" value="<%= TempatWisata.paket %>">
            <option value="Bisnis">Bisnis</option>
            <option value="Vip">Vip</option>
          </select>
        </div>
        <div class="form-group">
          <label for="">Harga</label>
          <input class="form-control" type="text" name="harga" value="<%= TempatWisata.harga %>">
        </div>
        <div class="form-group">
          <label for="">fasilitas</label>
          <input class="form-control" type="text" name="fasilitas" value="<%= TempatWisata.fasilitas %>">
        </div>
        <div class="form-group">
          <label for="">image</label>
          <input class="form-control type="text" name="image" value="<%= TempatWisata.image %>">
        </div>
        <button type="submit">submit</button>
      </form>
    </div>
  </div>
</div>
